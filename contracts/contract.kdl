// Apps.Fic.Kindelia (action: (Apps.Fic.Kindelia.Act)) : (Kindelia.IO (Apps.Fic.Kindelia.Response))
fun (Fic action) {
  (Fic {Fic_kdl_anxt code}) =
    {TAKE @state let blk = #0;
    let result = (Fic_kdl_supd state code blk);
    dup _0 _1 = result;
    let state = (Pair_fst _0);
    {SAVE state @~ {DONE {Fic_kdl_rnxt _1}}}}
  (Fic {Fic_kdl_actg}) =
    {TAKE @state dup _0 _1 = state;
    {SAVE _0 @~ {DONE {Fic_kdl_rst _1}}}}
} with {
    {Fic_kdl #0 (Fic_st_buld)}
}
